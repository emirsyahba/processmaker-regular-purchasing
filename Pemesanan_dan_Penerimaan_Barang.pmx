        <tri_title><![CDATA[[Before] Isi GR]]></tri_title>
        <tri_description></tri_description>
        <pro_uid>93698120166239192bcc973013147069</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[// Trigger untuk membersihkan field dan grid jika GR mengalami loop
if (@@status_gr == "Diterima Sebagian") {
    // Hapus isi field-field utama
    @@nomor_gr = '';
    @@pembuat_gr = '';
    @@tanggal_gr = '';
    @@gudang_terima_gr = '';
    @@nomor_faktur_supplier = '';
    @@catatan_gr = '';

    // Hapus isi kolom dalam grid items_of_po
    if (is_array(@@items_of_po)) {
        foreach (@@items_of_po as $index => $item) {
            @@items_of_po[$index]['kuantitas_diterima_gr'] = '';
        }
    }

    // Menghapus grid GR dan populate sisa barang yang harus diterima
    // Mendapatkan jumlah baris dalam grid
    @%rowCount = count(@=items_of_po);

    // Loop melalui setiap baris dalam grid
    for (@%i = 1; @%i <= @%rowCount; @%i++) {
        @%sisa = @=items_of_po[@%i]['kuantitas_sisa_gr'];
        
        // Memperbarui nilai totalKuantitas dengan nilai sisa
        @=items_of_po[@%i]['kuantitas_barang_pr'] = @%sisa;
    }
}

// Trigger untuk generate nomor GR dan mendapatkan tanggal
// Mendapatkan koneksi
$conn = "77755737366261ca34b5531012499436";

// Mendapatkan tanggal saat ini dalam format YYYY.MM.DD
$date = date("Y.m.d");

// Mengambil nomor GR terakhir dan tanggalnya dari database
$sql = executeQuery("SELECT no, tanggal_gr FROM gr_terbentuk ORDER BY no DESC LIMIT 1", $conn);
$lastNumber = $sql[1]['no'];
$lastDate = $sql[1]['tanggal_gr'];

// Mengekstrak tahun, bulan, dan hari dari tanggal saat ini
$currentYearMonthDay = date("Y.m.d");

// Mengekstrak tahun, bulan, dan hari dari tanggal GR terakhir
$lastYearMonthDay = date("Y.m.d", strtotime($lastDate));

// Memeriksa apakah tahun, bulan, dan hari saat ini berbeda dari tahun, bulan, dan hari terakhir
if ($currentYearMonthDay != $lastYearMonthDay) {
    // Jika tahun, bulan, dan hari berbeda, reset nomor urut menjadi 1
    $nextNumber = 1;
} else {
    // Jika tahun, bulan, dan hari sama, increment nomor urut
    $nextNumber = $lastNumber + 1;
}

// Increment nomor urut dan format dengan leading zeros
$nextSequentialNumber = str_pad($nextNumber, 2, '0', STR_PAD_LEFT);

// Membentuk nomor GR
$grNumber = 'GR.' . $date . "." . $nextSequentialNumber;

