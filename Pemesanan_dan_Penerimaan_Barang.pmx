        <tri_uid>202117919665bc79ee2dd14052493210</tri_uid>
        <tri_title><![CDATA[[After] Isi GR]]></tri_title>
        <tri_description></tri_description>
        <pro_uid>93698120166239192bcc973013147069</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[// Trigger untuk input data ke tabel gr_terbentuk dan produk_gr

// Mendapatkan koneksi
$connection = "77755737366261ca34b5531012499436";

// Mengambil variabel dari Dynaform
$noGR = @@nomor_gr;
$noPO = @@nomor_po;
$noPR = @@nomor_pr;
$pembuatGR = @@pembuat_gr;
$tanggalGR = @@tanggal_gr;
$gudangTerimaGR = @@gudang_terima_gr;
$nomorFakturSupplier = @@nomor_faktur_supplier;
$catatanGR = @@catatan_gr;
$currentTimestamp = date('Y-m-d H:i:s');

// SQL query untuk memasukkan data ke tabel gr_terbentuk
$query = "INSERT INTO gr_terbentuk (
    no_gr,
    no_po,
    no_pr,
    petugas_entri_gr,
    tanggal_gr,
    gudang_penerimaan,
    no_faktur_supplier,
    catatan_gr
) VALUES (
    '$noGR',
    '$noPO',
    '$noPR',
    '$pembuatGR',
    '$currentTimestamp',
    '$gudangTerimaGR',
    '$nomorFakturSupplier',
    '$catatanGR'
)";

// Eksekusi query
executeQuery($query, $connection);

// Mengambil variabel untuk grid produk_gr
$grid = @=items_of_po;
$rows = count($grid);

// Loop untuk setiap baris dalam grid dan memasukkan data ke tabel produk_gr
for($i = 1; $i <= $rows; $i++)
{
    $namaProduk = $grid[$i]["nama_barang_pr"];
    $kuantitasDiterima = $grid[$i]["kuantitas_diterima_gr"];
    $query = "INSERT INTO produk_gr (
        no_gr,
        no_po,
        no_pr,
        tanggal_gr,
        nama_barang,
        kuantitas_diterima
    ) VALUES (
        '$noGR',
        '$noPO',
        '$noPR',
        '$currentTimestamp',
        '$namaProduk',
        '$kuantitasDiterima'
    )";
    // Eksekusi query untuk setiap produk
    executeQuery($query, $connection);
}]]></tri_webbot>
