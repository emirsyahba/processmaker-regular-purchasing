        <dyn_content><![CDATA[{"name":"Purchase Order","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"722595214662eea7fa84b77015604136","name":"Purchase Order","description":"","mode":"edit","script":{"type":"js","code":"// Mendefinisikan fungsi untuk menghitung subtotal\nfunction calculateSubtotal() {\n    var sum = 0;\n    for (var i = 1; i <= $(\"#items_of_po\").getNumberRows(); i++) {\n        sum = sum + parseFloat($(\"#items_of_po\").getValue(i, 7));\n    }\n    $(\"#sub_total_po\").setValue(parseFloat(sum));\n}\n\n// Memanggil fungsi untuk menghitung subtotal saat halaman dimuat\ncalculateSubtotal();\n\n// Mendefinisikan fungsi untuk menghitung total harga\nfunction calculateTotal() {\n    var subTotal = parseFloat($(\"#sub_total_po\").getValue());\n    var diskon = parseFloat($(\"#diskon_from_subtotal_po\").getValue());\n    var ppn = parseFloat($(\"#ppn\").getValue());\n    var ppnbm = parseFloat($(\"#ppnbm\").getValue());\n    var otherCost = parseFloat($(\"#other_cost\").getValue());\n\n    var total = (subTotal - diskon) + ((subTotal - diskon) * (ppn / 100)) + ((subTotal - diskon) * (ppnbm / 100)) + otherCost;\n\n    $(\"#total_harga_po\").setValue(parseFloat(total));\n}\n\n// Memanggil fungsi untuk menghitung total setelah subtotal dihitung\ncalculateTotal();\n\n\n// Menjalankan fungsi ketika tombol di #update_total diklik\n$(\"#update_total\").find(\"button\").click(function() {\n    calculateSubtotal(); // Memanggil fungsi untuk menghitung subtotal\n    calculateTotal(); // Memanggil fungsi untuk menghitung total setelah subtotal dihitung\n});\n"},"language":"en","externalLibs":"","printable":false,"items":[[{"type":"title","id":"potittle","label":"Purchase Order","colSpan":12}],[{"type":"subtitle","id":"subtitle0000000001","label":"Keterangan PO","colSpan":12}],[{"type":"dropdown","variable":"nomor_pr","var_uid":"984497070662f420c917867010479270","dataType":"string","protectedValue":false,"id":"nomor_pr","name