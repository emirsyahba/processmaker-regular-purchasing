        <tri_title><![CDATA[[After] Buat dokumen PO]]></tri_title>
        <tri_description></tri_description>
        <pro_uid>93698120166239192bcc973013147069</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[// Memformat tanggal untuk output dokumen PO
$tanggal_po = @@tanggal_po;
$tanggal_pengiriman_po = @@tanggal_pengiriman_po;

// Memformat tanggal menjadi DD MM YYYY
$formatted_tanggal_po = date("d M Y", strtotime($tanggal_po));
$formatted_tanggal_pengiriman_po = date("d M Y", strtotime($tanggal_pengiriman_po));

// Menyimpan tanggal yang telah diformat ke variabel baru
@@formatted_tanggal_po = $formatted_tanggal_po;
@@formatted_tanggal_pengiriman_po = $formatted_tanggal_pengiriman_po;

// Memasukkan data PO yang terbentuk ke dalam tabel po_terbentuk
$connection = "77755737366261ca34b5531012499436";
$noPO = @@nomor_po;
$noPR = @@nomor_pr;
$pembuatPO = @@pembuat_po;
$tanggalPO = @@tanggal_po;
$tanggalKirimPO = @@tanggal_pengiriman_po;
$supplierPO = @@supplier_po;
$alamatSupplierPO = @@alamat_supplier_po;
$tipePembayaran = @@pembayaran_supplier_po;
$subTotal = @@sub_total_po;
$diskonSubTotal = @@diskon_from_subtotal_po;
$ppn = @@ppn;
$ppnbm = @@ppnbm;
$biayaLain = @@other_cost;
$totalBiaya = @@total_harga_po;
$deskripsiPO = @@keterangan_po;
$currentTimestamp = date('Y-m-d H:i:s');

$query = "INSERT INTO po_terbentuk (
    no_po,
    no_pr,
    pembuat_po,
    tanggal_terbentuk_po,
    tanggal_pengiriman_po,
    supplier_po,
    alamat_supplier_po,
    tipe_pembayaran_po,
    sub_total,
    diskon_sub_total,
    ppn,
    ppnbm,
    biaya_lain,
    total,
    deskripsi_po,
    status_po
) VALUES (
    '$noPO',
    '$noPR',
    '$pembuatPO',
    '$currentTimestamp',
    '$tanggalKirimPO',
    '$supplierPO',
    '$alamatSupplierPO',
    '$tipePembayaran',
    '$subTotal',
    '$diskonSubTotal',
    '$ppn',
    '$ppnbm',
    '$biayaLain',
    '$totalBiaya',
    '$deskripsiPO',
    'Dibuat'
)";
executeQuery($query, $connection);

// Memasukkan data baris barang PO ke dalam tabel produk_po
$connection = "77755737366261ca34b5531012499436";
$noPO = @@nomor_po;
$noPR = @@nomor_pr;
$grid = @=items_of_po;
$rows = count($grid);

for($i = 1; $i <= $rows; $i++) {
    $namaProduk = $grid[$i]["nama_barang_pr"];
    $kuantitasProduk = $grid[$i]["kuantitas_barang_pr"];
    $hargaProduk = $grid[$i]["harga_barang_terpilih"];
    $tanggalDiminta = $grid[$i]["tanggal_diminta_pr"];
    $diskonMaterial = $grid[$i]["default_diskon"];
    $jumlah = $grid[$i]["jumlah"];

    $query = "INSERT INTO produk_po (
        no_po,
        no_pr,
        nama_barang_po,
        kuantitas_barang_po,
        harga_unit,
        tanggal_diminta_po,
        diskon_material,
        jumlah,
        status_barang_po
    ) VALUES (
        '$noPO',
        '$noPR',
        '$namaProduk',
        '$kuantitasProduk',
        '$hargaProduk',
        '$tanggalDiminta',
        '$diskonMaterial',
        '$jumlah',
        'Dibuat'
    )";
    executeQuery($query, $connection);
}]]></tri_webbot>
