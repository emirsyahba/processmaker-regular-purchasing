      <record>
        <tri_uid>128845214665bc491e593b3018719564</tri_uid>
        <tri_title><![CDATA[[Before] Buat dokumen PO]]></tri_title>
        <tri_description></tri_description>
        <pro_uid>93698120166239192bcc973013147069</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[// Memperoleh connection
$conn = "77755737366261ca34b5531012499436";

// Mendapatkan tanggal saat ini dalam format YYYY.MM.DD
$date = date("Y.m.d");

// Mengambil nomor PO terakhir dan tanggalnya dari database
$sql = executeQuery("SELECT no, tanggal_terbentuk_po FROM po_terbentuk ORDER BY no DESC LIMIT 1", $conn);
$lastNumber = $sql[1]['no'];
$lastDate = $sql[1]['tanggal_terbentuk_po'];

// Mengekstrak tahun, bulan, dan hari dari tanggal saat ini
$currentYearMonthDay = date("Y.m.d");

// Mengekstrak tahun, bulan, dan hari dari tanggal PO terakhir
$lastYearMonthDay = date("Y.m.d", strtotime($lastDate));

// Memeriksa apakah tahun, bulan, dan hari saat ini berbeda dari tahun, bulan, dan hari terakhir
if ($currentYearMonthDay != $lastYearMonthDay) {
    // Jika tahun, bulan, dan hari saat ini berbeda, mengatur nomor urut ke 1
    $nextNumber = 1;
} else {
    // Jika tahun, bulan, dan hari sama, nomor urut ditingkatkan
    $nextNumber = $lastNumber + 1;
}

// Meningkatkan nomor urut dan memformat dengan nol di depan
$nextSequentialNumber = str_pad($nextNumber, 2, '0', STR_PAD_LEFT);

// Membentuk nomor PO
$poNumber = 'PO.' . $date . "." . $nextSequentialNumber;

// Mengatur nomor PO ke variabel ProcessMaker
@@nomor_po = $poNumber;

// Mengatur tanggal saat ini ke variabel ProcessMaker untuk field dynaform
@@tanggal_po = date("Y-m-d");

// Mengambil nomor PR
$nomorPR = @@nomor_pr;

// Menentukan connection
$connection = "77755737366261ca34b5531012499436";

// Query untuk mengambil data dari grid PR dan mengisinya ke grid PO
$query = "SELECT supplier_barang_terpilih, nama_barang_pr, kuantitas_barang_pr, harga_barang_terpilih, tanggal_diminta_pr, default_diskon FROM produk_pr WHERE no_pr='$nomorPR'";
$result = executeQuery($query, $connection);
if (is_array($result) and count($result) > 0)
    @=items_of_po = $result;

// Mengambil informasi pengguna yang membuat PO
$user_info = userInfo(@@USER_LOGGED);
@@pembuat_po = $user_info['firstname'] . ' ' . $user_info['lastname'];
@@requester = @@USER_LOGGED;]]></tri_webbot>
