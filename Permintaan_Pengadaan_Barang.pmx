        <pro_uid>593170166662e035004da83029718999</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[// Menggabungkan kedua trigger

// Create connection
$connection = "182220458662e050d733249066070611";

// Ambil data dari variabel ProcessMaker
$noRFQ = @@nomor_rfq;
$seleksiSupplier = @@keterangan_supplier;
$tipePembayaranSupplier = @@tipe_pembayaran_supplier;
$nomorPR = @@nomor_pr;
$supplierPR = @@supplier_quo;
$currentTimestamp = date('Y-m-d H:i:s');

// Bagian 1: Update tabel rfq_terbentuk dan produk_rfq untuk proses
$query1 = "UPDATE rfq_terbentuk SET seleksi_supplier = '$seleksiSupplier', status_rfq = 'Diproses', tanggal_isi_quotation = '$currentTimestamp' WHERE no_rfq = '$noRFQ'";
executeQuery($query1, $connection);

// Update tabel produk_rfq
$grid = @=items_of_quo;
$rows = count($grid);
for ($i = 1; $i <= $rows; $i++) {
    $hargaBarangRFQ = $grid[$i]["harga_barang_quo"];
    $query2 = "UPDATE produk_rfq SET status_barang_rfq = 'Diproses', harga_barang_rfq = '$hargaBarangRFQ' WHERE no_rfq = '$noRFQ'";
    executeQuery($query2, $connection);
}

// Update tabel supplier_rfq_sementara
$query3 = "UPDATE supplier_rfq_sementara SET tipe_pembayaran_supplier = '$tipePembayaranSupplier' WHERE no_rfq = '$noRFQ'";
executeQuery($query3, $connection);

// Bagian 2: Kondisi khusus untuk update produk_pr dan status tabel jika @@keterangan_supplier == 'Tidak'
if (@@keterangan_supplier == 'Tidak') {
    // Update tabel produk_pr
    for ($i = 1; $i <= $rows; $i++) {
        $namaBarangRFQ = $grid[$i]["nama_barang_rfq"];
        $hargaBarangRFQ = $grid[$i]["harga_barang_quo"];
        $query4 = "UPDATE produk_pr SET harga_barang_terpilih = '$hargaBarangRFQ', supplier_barang_terpilih = '$supplierPR' WHERE no_pr = '$nomorPR' AND nama_barang_pr = '$namaBarangRFQ'";
        executeQuery($query4, $connection);
    }

    // Update status tabel rfq_terbentuk menjadi 'Diterima'
    $query5 = "UPDATE rfq_terbentuk SET seleksi_supplier = '$seleksiSupplier', tanggal_evaluasi_quotation = '$currentTimestamp', status_rfq = 'Diterima' WHERE no_rfq = '$noRFQ'";
    executeQuery($query5, $connection);

    // Update status tabel produk_rfq menjadi 'Diterima'
    for ($i = 1; $i <= $rows; $i++) {
        $hargaBarangRFQ = $grid[$i]["harga_barang_quo"];
        $query6 = "UPDATE produk_rfq SET status_barang_rfq = 'Diterima', harga_barang_rfq = '$hargaBarangRFQ' WHERE no_rfq = '$noRFQ'";
        executeQuery($query6, $connection);
    }

    // Update status tabel supplier_rfq_sementara menjadi 'Diterima'
    $query7 = "UPDATE supplier_rfq_sementara SET tipe_pembayaran_supplier = '$tipePembayaranSupplier', status_supplier = 'Diterima' WHERE no_rfq = '$noRFQ'";
    executeQuery($query7, $connection);
}]]></tri_webbot>
