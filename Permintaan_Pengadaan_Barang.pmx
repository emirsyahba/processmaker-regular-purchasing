        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[$connection = "182220458662e050d733249066070611";
$noRFQ = @@nomor_rfq;
$noPR = @@nomor_pr;
$tanggalTerbentukRFQ = @@tanggal_rfq;
$tanggalValidRFQ = @@tanggal_valid_rfq;
$pembuatRFQ = @@pembuat_rfq;
$supplierRFQ = @@supplier_rfq;
$keteranganRFQ = @@keterangan_rfq;
$idSupplier = @@id_supplier_rfq;
$kategoriSupplier = @@kategori_supplier_rfq;
$jalanSupplier = @@jalan_supplier_rfq;
$kotaSupplier = @@kota_supplier_rfq;
$provinsiSupplier = @@provinsi_supplier_rfq;
$negaraSupplier = @@negara_supplier_rfq;
$kodePosSupplier = @@kode_pos_supplier_rfq;
$currentTimestamp = date('Y-m-d H:i:s');

// Query untuk INSERT data ke dalam tabel rfq_terbentuk
$queryRFQ = "INSERT INTO rfq_terbentuk (
    no_rfq,
	no_pr,
	tanggal_terbentuk_rfq,
	tanggal_valid_rfq,
	pembuat_rfq,
	supplier_rfq,
	keterangan_rfq,
	status_rfq
) VALUES (
    '$noRFQ',
    '$noPR',
    '$currentTimestamp',
    '$tanggalValidRFQ',
	'$pembuatRFQ',
	'$supplierRFQ',
	'$keteranganRFQ',
	'Dibuat'
)";
executeQuery($queryRFQ, $connection);

// Query untuk INSERT data ke dalam tabel supplier_rfq_sementara
$querySupplier = "INSERT INTO supplier_rfq_sementara (
    no_rfq,
	id_supplier,
	nama_supplier,
	kategori_supplier,
	jalan_supplier,
	kota_supplier,
	provinsi_supplier,
	negara_supplier,
	kode_pos_supplier,
	status_supplier
) VALUES (
    '$noRFQ',
    '$idSupplier',
    '$supplierRFQ',
    '$kategoriSupplier',
	'$jalanSupplier',
	'$kotaSupplier',
	'$provinsiSupplier',
	'$negaraSupplier',
	'$kodePosSupplier',
	'Negosiasi'
)";
executeQuery($querySupplier, $connection);

// Mendapatkan data dari grid items_of_rfq
$grid = @=items_of_rfq;
$rows = count($grid);

// Loop untuk insert ke tabel produk_rfq
for ($i = 1; $i <= $rows; $i++) {
    $namaBarangRFQ = $grid[$i]["nama_barang_pr"];
    $kuantitasBarangRFQ = $grid[$i]["kuantitas_barang_pr"];
    $satuanBarangRFQ = $grid[$i]["satuan_barang_pr"];
    $tanggalDimintaRFQ = $grid[$i]["tanggal_diminta_pr"];
    
    $queryProdukRFQ = "INSERT INTO produk_rfq (
        no_rfq,
        no_pr,
        supplier_barang_rfq,
        nama_barang_rfq,
        kuantitas_barang_rfq,
        satuan_barang_rfq,
        tanggal_diminta_rfq,
        status_barang_rfq
    ) VALUES (
        '$noRFQ',
        '$noPR',
        '$supplierRFQ',
        '$namaBarangRFQ',
        '$kuantitasBarangRFQ',
        '$satuanBarangRFQ',
        '$tanggalDimintaRFQ',
        'Dibuat'
    )";
    executeQuery($queryProdukRFQ, $connection);
}

// Ubah format tanggal
$tanggal_rfq = @@tanggal_rfq;
$tanggal_valid_rfq = @@tanggal_valid_rfq;

// Format the dates to DD MM YYYY
$formatted_tanggal_rfq = date("d M Y", strtotime($tanggal_rfq));
$formatted_tanggal_valid_rfq = date("d M Y", strtotime($tanggal_valid_rfq));

// Assign the formatted dates to new variables
@@formatted_tanggal_rfq = $formatted_tanggal_rfq;
@@formatted_tanggal_valid_rfq = $formatted_tanggal_valid_rfq;

// Concat Alamat Supplier
$jalan = @@jalan_supplier_rfq;
$kota = @@kota_supplier_rfq;
$provinsi = @@provinsi_supplier_rfq;
$negara = @@negara_supplier_rfq;
$kode_pos = @@kode_pos_supplier_rfq;

// Menggabungkan nilai-nilai tersebut menjadi satu string alamat
$alamat_supplier_rfq = "$jalan, $kota, $provinsi, $negara, $kode_pos";

// Menyimpan hasil gabungan ke dalam variabel @@alamat_supplier_rfq
@@alamat_supplier_rfq = $alamat_supplier_rfq;
]]></tri_webbot>
