        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[$connection = "182220458662e050d733249066070611";
$currentTimestamp = date('Y-m-d H:i:s');
$grid = @=evaluasi_rfq;
$rows = count($grid);

for ($i = 1; $i <= $rows; $i++) {
    $noRFQ = $grid[$i]["no_rfq"];
    $keputusanRFQ = $grid[$i]["keputusan_rfq"];
    
    // Update rfq_terbentuk
    $query1 = "UPDATE rfq_terbentuk 
               SET status_rfq = '".$keputusanRFQ."', 
                   tanggal_evaluasi_quotation = '$currentTimestamp' 
               WHERE no_rfq = '".$noRFQ."'";
    executeQuery($query1, $connection);
    
    // Update produk_rfq
    $query2 = "UPDATE produk_rfq 
               SET status_barang_rfq = '".$keputusanRFQ."' 
               WHERE no_rfq = '".$noRFQ."'";
    executeQuery($query2, $connection);
	
    // Update supplier_rfq_sementara
    $query3 = "UPDATE supplier_rfq_sementara 
               SET status_supplier = '".$keputusanRFQ."' 
               WHERE no_rfq = '".$noRFQ."'";
    executeQuery($query3, $connection);
}]]></tri_webbot>
