        <dyn_content><![CDATA[{"name":"Dokumen Perencanaan Pembelian","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"374328613663e235f304853038467020","name":"Dokumen Perencanaan Pembelian","description":"","mode":"edit","script":{"type":"js","code":"// Fungsi untuk menghitung dan memperbarui kolom total di grid\nfunction updateTotalPrice() {\n    var kursDollar = parseFloat($(\"#kurs_dollar\").getValue()); // Mendapatkan nilai dari field kurs_dollar\n\n    if (isNaN(kursDollar)) {\n        alert(\"Nilai kurs dollar tidak valid!\");\n        return;\n    }\n\n    var numberOfRows = $(\"#rencana_po\").getNumberRows(); // Mendapatkan jumlah baris di grid rencana_po\n\n    // Loop melalui setiap baris di grid\n    for (var i = 1; i <= numberOfRows; i++) {\n        var jumlah = parseFloat($(\"#rencana_po\").getValue(i, 7)); // Mendapatkan nilai dari kolom jumlah (kolom ke-7) di baris ke-i\n\n        if (isNaN(jumlah)) {\n            alert(\"Nilai jumlah pada baris ke-\" + i + \" tidak valid!\");\n            continue;\n        }\n\n        var total = jumlah * kursDollar; // Menghitung total\n        $(\"#rencana_po\").setValue(total.toFixed(2), i, 8); // Mengatur nilai kolom total (kolom ke-8) di baris ke-i\n    }\n}\n\n// Menjalankan fungsi ketika tombol dengan id update_price_button diklik\n$(\"#update_price_button\").on(\"click\", function() {\n    updateTotalPrice(); // Memanggil fungsi untuk memperbarui kolom total di grid\n});\n"},"language":"en","externalLibs":"","printable":false,"items":[[{"type":"title","id":"title0000000001","label":"Perencanaan Pembelian atas Purchase Requesition","colSpan":12}],[{"type":"subtitle","id":"subtitle0000000001","label":"Informasi Purchase Requisition","colSpan":12}],[{"type":"text","variable":"nomor_pr","var_uid":"747589111662e09b21f4276001035733","dataType":"string","protectedValue":false,"id":"nomor_pr","name":"nomor_pr","label":"No. PR","defaultValue":"","placeholder":"","hint":"","re