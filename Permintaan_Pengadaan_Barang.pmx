        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[$connection = "182220458662e050d733249066070611";
$noPR = @@nomor_pr;
$tanggalPR = @@tanggal_pr;
$currentTimestamp = date('Y-m-d H:i:s');
$pemohonPR = @@pemohon_pr;
$kurs= @@kurs_dollar;
$grid = @=rencana_po;
$rows = count($grid);

// Insert to rencana_pembelian
for($i = 1; $i <= $rows; $i++) {
    $supplier = $grid[$i]["supplier_barang_terpilih"];
    $area = $grid[$i]["area"];
    $unit = $grid[$i]["nama_barang_pr"];
    $cbm = $grid[$i]["cbm"];
    $kuantitas = $grid[$i]["kuantitas_barang_pr"];
    $hargaDollar = $grid[$i]["harga_barang_terpilih"];
    $jumlahDollar = $grid[$i]["jumlah"];
    $totalRP = $grid[$i]["total"];
    
    $query1 = "INSERT INTO rencana_pembelian_po (
        no_pr,
        tanggal_perencanaan,
        pemohon_pr,
        supplier,
        area,
        nama_barang_pr,
        cbm,
        kuantitas_barang_pr,
        harga_barang_dollar,
        jumlah_dollar,
        kurs,
        jumlah_rp
    ) VALUES (
        '$noPR',
        '$currentTimestamp',
        '$pemohonPR',
        '$supplier',
        '$area',
        '$unit',
        '$cbm',
        '$kuantitas',
        '$hargaDollar',
        '$jumlahDollar',
        '$kurs',
        '$totalRP'
    )";
    executeQuery($query1, $connection);
	
	$query2 = "UPDATE produk_pr SET harga_barang_terpilih = '" . $hargaDollar . "' WHERE no_pr = '" . $noPR . "' AND nama_barang_pr = '" . $unit . "'";
    executeQuery($query2, $connection);
}

]]></tri_webbot>
